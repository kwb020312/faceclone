        posting_bottom_newspeed.addEventListener('click',() => {
            if(displaying_checkbox.className === 'circle_no_check') {
                displaying_checkbox.className = 'circle_check';
                checking.style.display = 'block';
            } else {
                displaying_checkbox.className = 'circle_no_check';
                checking.style.display = 'none';
            }
        });

        posting_bottom_mystory.addEventListener('click',() => {
            if(displaying_checkbox2.className === 'circle_no_check') {
                displaying_checkbox2.className = 'circle_check';
                checking2.style.display = 'block';
            } else {
                displaying_checkbox2.className = 'circle_no_check';
                checking2.style.display = 'none';
            }
        });


        top_line_friend.addEventListener('click',() => {
            top_line_friend.style.color= 'white';
        });
        top_line_messenger.addEventListener('click',() => {
            top_line_messenger.style.color= 'white';
        });
        top_line_bell.addEventListener('click',() => {
            top_line_bell.style.color= 'white';
        });
        top_line_question.addEventListener('click',() => {
            top_line_question.style.color= 'white';
        });
        top_line_menu.addEventListener('click',() => {
            top_line_menu.style.color= 'white';
        });
        left_nav_user.addEventListener('mouseover',() => {
            left_nav_user_menu.style.display='inline-block';
        })
        left_nav_user.addEventListener('mouseout',() => {
            left_nav_user_menu.style.display='none';
        })
        first_story.addEventListener('mouseover',() => {
            darkness.style.opacity = '0.1';
            mid_nav_story_firstStory_image.style.transform = 'scale(1.05)';
        });
        first_story.addEventListener('mouseout',() => {
            darkness.style.opacity = '0';
            mid_nav_story_firstStory_image.style.transform = 'scale(1)';
        });
        mid_top_nav_input.addEventListener('click',() => {
            body_black.style.display = 'block';
            posting.style.display = 'block';
            mid_nav_story.style.transform = 'translateX(638px) translateY(255px)';
            mid_nav_desc.style.transform = 'translateX(638px) translateY(265px)';
        });
        body_black.addEventListener('click',() => {
            body_black.style.display = 'none';
            posting.style.display = 'none';
            mid_nav_story.style.transform = 'translateX(638px) translateY(66px)';
            mid_nav_desc.style.transform = 'translateX(638px) translateY(76px)';
        })
    </script>
</body>
