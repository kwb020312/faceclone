        // body 전체
        var body = document.querySelector('body');

        // 왼쪽 위 페이스북 로고 혹은 홈 div 를 클릭했을 경우 홈으로 감

        for(let i = 0 ; i < go_home.length ; i ++) {
            go_home[i].addEventListener('click',(e) => {
                location.href = '/';
            })
        }


        // 댓글 버튼을 눌렀을 때의 이벤트처리
        for(let i = 0 ; i < mid_nav_desc_write_comment.length; i++) {
            mid_nav_desc_write_comment[i].addEventListener('click',(e) => {
                if(comment_big_box[i].style.display === 'block') {
                    comment_big_box[i].style.display = 'none';
                } else {
                    comment_big_box[i].style.display = 'block';
                }
            })
        }

        // 댓글작성 엔터누를경우 /comment 로 전송
        for(let i = 0 ; i < comment_chat_box_input.length ; i ++) {
            comment_chat_box_input[i].addEventListener('keypress',(e) => {
                if(e.code === 'Enter') {
                    console.log('how entered');
                    comment_form[i].submit();
                    var mid_nav_comment_box_eachbox = document.createElement('div');
                    mid_nav_comment_box_eachbox.className = 'mid_nav_comment_box_eachbox';
                    mid_nav_comment_box[i].appendChild(mid_nav_comment_box_eachbox);

                    var mid_nav_comment_profile = document.createElement('div');
                    mid_nav_comment_profile.className = 'mid_nav_comment_profile';
                    mid_nav_comment_box_eachbox.appendChild(mid_nav_comment_profile);

                    var mid_nav_comment_chatbox = document.createElement('div');
                    mid_nav_comment_chatbox.className = 'mid_nav_comment_chatbox';
                    mid_nav_comment_chatbox.classList.add('chat_box_margin_left');
                    mid_nav_comment_box_eachbox.appendChild(mid_nav_comment_chatbox);

                    var mid_nav_comment_chat = document.createElement('p');
                    mid_nav_comment_chat.className = 'mid_nav_comment_chat';
                    mid_nav_comment_chat.innerText = comment_chat_box_input[i].value;
                    mid_nav_comment_chatbox.appendChild(mid_nav_comment_chat);

                    comment_chat_box_input[i].value = '';
                }
            })
        }

        for(let i = 0 ; i < comments.length ; i ++) {
            comments[i].addEventListener('click',() => {
                alert('clicked!');
            })
        }

        for(let i = 0 ; i < mid_nav_desc_good.length ; i ++) {
            mid_nav_desc_good[i].addEventListener('click',() => {
                // 좋아요버튼 색이 칠해져있지 않다면,
                if(mid_nav_desc_good_icon[i].style.color !== 'rgb(32, 120, 244)') {
                    // 좋아요 처리
                    mid_nav_desc_good_icon[i].style.color= 'rgb(32, 120, 244)';
                    mid_nav_desc_good_p[i].style.color= 'rgb(32, 120, 244)';
                        // 좋아요 수 에 ~ 님 외 N명 의 값을 넣음
                    mid_nav_desc_feeling_thumb_count[i].innerText = '회원님, 외 '+mid_nav_desc_feeling_thumb_count[i].innerText+'명';   
                    like_count[i].submit();
                } else {
                    // 이미 좋아요 누른 게시물은 꾸미도 다시 클릭시 좋아요취소 처리
                    mid_nav_desc_good_icon[i].style.color= 'gray';
                    mid_nav_desc_good_p[i].style.color= 'black';
                    mid_nav_desc_feeling_thumb_count[i].innerText = mid_nav_desc_feeling_thumb_count[i].innerText.substring(7,mid_nav_desc_feeling_thumb_count[i].innerText.length - 1);
                    un_like_count[i].submit();
                }
            });
        }


        posting_admit.addEventListener('click',() => {
            user_post.submit();
        })

        user_posting.addEventListener('keyup',(e) => {
            if(e.target.value.length !== 0) {
                posting_admit.style.backgroundColor= 'rgba(57,92,159)';
            } else {
                posting_admit.style.backgroundColor= 'rgba(156,180,216)';
            }
        });

        posting_bottom_newspeed.addEventListener('click',() => {
            if(displaying_checkbox.className === 'circle_no_check') {
                displaying_checkbox.className = 'circle_check';
                checking.style.display = 'block';
            } else {
                displaying_checkbox.className = 'circle_no_check';
                checking.style.display = 'none';
            }
        });

        posting_bottom_mystory.addEventListener('click',() => {
            if(displaying_checkbox2.className === 'circle_no_check') {
                displaying_checkbox2.className = 'circle_check';
                checking2.style.display = 'block';
            } else {
                displaying_checkbox2.className = 'circle_no_check';
                checking2.style.display = 'none';
            }
        });

        // body 클릭 전체관리

        body.addEventListener('click' , ( e ) => {
            var opend = false;
            // 만약 상단 친구버튼의 활성화 시에,
            if(top_line_friend.style.color === 'white') {
                // 지금 클릭한 body 의 className 이 f 로 시작하지 않는경우
                if(e.target.className[0] !== 'f') {
                    // 비활성화 시킨다
                    top_line_friend.style.color= 'black';
                    // 상단의 친구 버튼을 눌렀을 경우
                    friend_div.style.display = 'none';
                    friend_div_top_triangle.style.display = 'none';
                } else {
                  
                }
            }
            // console.log(e.target.className[0]);
            // 만약 상단 메신저버튼의 활성화 시에,
            if(top_line_messenger.style.color === 'white') {
                // 지금 클릭한 body 의 className 이 f 로 시작하지 않는경우
                if(e.target.className[0] !== 'f') {
                    // 비활성화 시킨다
                    top_line_messenger.style.color= 'black';
                    // 상단의 친구 버튼을 눌렀을 경우
                    messenger_div.style.display = 'none';
                    messenger_div_top_triangle.style.display = 'none';
                } else {

                }
            }
        });

        top_line_friend.addEventListener('click',() => {
            if(top_line_friend.style.color === 'white') {
                top_line_friend.style.color= 'black';
                // 상단의 친구 버튼을 눌렀을 경우
                friend_div.style.display = 'none';
                friend_div_top_triangle.style.display = 'none';
            } else {
                top_line_friend.style.color= 'white';
                // 상단의 친구 버튼을 눌렀을 경우
                friend_div.style.display = 'block';
                friend_div_top_triangle.style.display = 'inline-block';
                // 메신저 버튼의 display 를 다 없애기
                top_line_messenger.style.color= 'black';
                // 상단의 메신저 버튼을 눌렀을 경우
                messenger_div.style.display = 'none';
                messenger_div_top_triangle.style.display = 'none';
            }
            
        });

        // 상단의 메신저 버튼을 누른경우,
        top_line_messenger.addEventListener('click',() => {
            if(top_line_messenger.style.color === 'white') {
                top_line_messenger.style.color= 'black';
                // 상단의 메신저 버튼을 눌렀을 경우
                messenger_div.style.display = 'none';
                messenger_div_top_triangle.style.display = 'none';
            } else {
                top_line_messenger.style.color= 'white';
                // 상단의 메신저 버튼을 눌렀을 경우
                messenger_div.style.display = 'block';
                messenger_div_top_triangle.style.display = 'inline-block';
                // 나머지 버튼의 display 를 안보이게 하기
                top_line_friend.style.color= 'black';
                // 상단의 친구 버튼을 눌렀을 경우
                friend_div.style.display = 'none';
                friend_div_top_triangle.style.display = 'none';
            }
        });
        top_line_bell.addEventListener('click',() => {
            top_line_bell.style.color= 'white';
        });
        top_line_question.addEventListener('click',() => {
            top_line_question.style.color= 'white';
        });
        top_line_menu.addEventListener('click',() => {
            top_line_menu.style.color= 'white';
        });
        left_nav_user.addEventListener('mouseover',() => {
            left_nav_user_menu.style.display='inline-block';
        })
        left_nav_user.addEventListener('mouseout',() => {
            left_nav_user_menu.style.display='none';
        })
        first_story.addEventListener('mouseover',() => {
            darkness.style.opacity = '0.1';
            mid_nav_story_firstStory_image.style.transform = 'scale(1.05)';
        });
        first_story.addEventListener('mouseout',() => {
            darkness.style.opacity = '0';
            mid_nav_story_firstStory_image.style.transform = 'scale(1)';
        });
        mid_top_nav_input.addEventListener('click',() => {
            body_black.style.display = 'block';
            posting.style.display = 'block';
            mid_nav_story.style.transform = 'translateX(638px) translateY(255px)';
            mid_nav_desc.style.marginTop = '190px';
            no_space.style.transform = 'translateY(265px)';
        });
        body_black.addEventListener('click',() => {
            body_black.style.display = 'none';
            posting.style.display = 'none';
            mid_nav_story.style.transform = 'translateX(638px) translateY(66px)';
            mid_nav_desc.style.marginTop = '0px';
            no_space.style.transform = 'translateY(0px)';
        })
    </script>
</body>
</html>